{
  "version": 2,
  "functions": {
    "api/app.py": {
      "runtime": "python3.11"
    }
  },
  "routes": [
    { 
      "src": "/api/(.*)", 
      "dest": "api/app.py" 
    },
    { 
      "src": "/(.*)", 
      "dest": "frontend/$1" 
    }
  ],
  "env": {
    "BACKEND_URL": "@backend_url"
  }
} 